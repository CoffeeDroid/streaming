<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_pressed="true">
        <shape>
            <gradient android:startColor="#ff53bbe2" android:endColor="#ff538afb" android:angle="90" android:type="linear"/>
            <corners android:radius="3dp"/>
            <stroke android:width="1dp" android:color="#ff3490e8"/>
        </shape>
    </item>
    <item>
        <shape>
            <gradient android:startColor="#ff53bbe2" android:endColor="#ff538afb" android:angle="270" android:type="linear"/>
            <corners android:radius="3dp"/>
            <stroke android:width="1dp" android:color="#ff3490e8"/>
        </shape>
    </item>
</selector>
