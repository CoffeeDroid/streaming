<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_pressed="true">
        <shape>
            <gradient android:startColor="#ffac4056" android:endColor="#ffbc3143" android:angle="270"/>
            <corners android:radius="40dp"/>
        </shape>
    </item>
    <item android:state_enabled="true">
        <shape android:shape="rectangle">
            <gradient android:startColor="#ffff7390" android:endColor="#ffff415a" android:angle="270"/>
            <corners android:radius="40dp"/>
        </shape>
    </item>
</selector>
