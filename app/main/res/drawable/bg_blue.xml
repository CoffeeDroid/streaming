<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#ff123b69"/>
            <corners android:bottomLeftRadius="5dp" android:bottomRightRadius="5dp"/>
        </shape>
    </item>
    <item>
        <shape>
            <stroke android:width="1dp" android:color="#ff1789df"/>
            <gradient android:startColor="#ff509df2" android:endColor="#ff3eadfd" android:angle="0"/>
            <corners android:bottomLeftRadius="5dp" android:bottomRightRadius="5dp"/>
        </shape>
    </item>
</selector>
